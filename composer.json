{
  "name": "thl/composer-installer",
  "description": "Composer installer for modules.",
  "keywords": [
    "composer-installer" , "thl"
  ],
  "minimum-stability": "stable",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Sunel TR",
      "email": "sunelbe@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "composer-plugin-api": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "THL\\Installer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "THL\\Installer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "THL\\Installer\\Plugin"
  }
}